{
  "name": "rdx",
  "version": "4.11.3",
  "description": "An HTML/ES/JSX compiler.",
  "main": "index.js",
  "bin": {
    "rdx": "src/index.js"
  },
  "watch": {
    "test": {
      "patterns": [
        "src"
      ],
      "extensions": "jsx,spec.jsx"
    }
  },
  "scripts": {
    "test": "mocha --require resistdesign-babel-register --ui exports **/*.spec.jsx",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resistdesign/rdx.git"
  },
  "keywords": [
    "javascript",
    "html",
    "webpack",
    "es6",
    "es7",
    "es2015",
    "es2016",
    "jsx",
    "react",
    "compile",
    "compiler"
  ],
  "author": "Resist Design",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/resistdesign/rdx/issues"
  },
  "homepage": "http://resistdesign.github.io/rdx",
  "dependencies": {
    "@resistdesign/jsx-compiler": "^2.3.0",
    "cheerio": "^1.0.0-rc.3",
    "commander": "^4.1.1",
    "css-loader": "^3.4.2",
    "file-loader": "^5.1.0",
    "webpack": "^4.41.6"
  },
  "devDependencies": {
    "expect.js": "^0.3.1",
    "flow-bin": "^0.120.1",
    "mocha": "^7.1.0",
    "npm-watch": "^0.6.0",
    "resistdesign-babel-register": "^3.1.0"
  },
  "files": [
    "src"
  ],
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=6.0.0"
  }
}
